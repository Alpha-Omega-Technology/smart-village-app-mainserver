<h1>PushNotification Logs</h1>
<p>Daten werden nach 2 Stunden automatisch gel√∂scht</p>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Date</th>
      <th>Messages Title</th>
      <th>Messages Body</th>
      <th>Messages Data</th>
      <th>Payload</th>
    </tr>
  </thead>
  <tbody>
    <% @push_logs.each do |message| %>
      <% begin
            serialized_message = JSON.parse(message)
          rescue
            serialized_message = {}
          end
      %>
      <tr>
        <td><%= serialized_message["date"] %></td>
        <td><%= serialized_message["title"] %></td>
        <td><%= serialized_message["body"] %></td>
        <td><%= serialized_message["data"] %></td>
        <td><%= serialized_message["payload"] %></td>
      </tr>
    <% end %>
  </tbody>
</table>
